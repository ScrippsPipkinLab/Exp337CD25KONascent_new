cvt_names <- function(vec_x, in_vec, out_vec){
vec_x_out <- c()
for (i in vec_x) {
if (i %in% in_vec){
i_idx <- match(i, in_vec)
vec_x_out <- c(vec_x_out,out_vec[i_idx])
} else {
vec_x_out <- c(vec_x_out, i)
}
}
return(vec_x_out)
}
